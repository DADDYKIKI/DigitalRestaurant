<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="200dp"
    android:layout_height="380dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="200dp"
        android:layout_height="380dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="196dp"
            android:layout_height="380dp"
            android:background="@drawable/adaguinessbeer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="91dp"
                android:layout_height="29dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/homekeyback"
                android:text="Help"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.357"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="91dp"
                android:layout_height="29dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/homekeyback"
                android:text="Contacts"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.357"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <TextView
                android:id="@+id/basket"
                android:layout_width="91dp"
                android:layout_height="29dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/homekeyback"
                android:text="Basket"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.357"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/logout"
                android:layout_width="91dp"
                android:layout_height="29dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/homekeyback"
                android:text="Logout"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.357"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/basket" />

            <TextView
                android:id="@+id/logout2"
                android:layout_width="129dp"
                android:layout_height="52dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/homekeyback"
                android:text="Find a restaurants"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="18dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.507"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/logout3" />

            <TextView
                android:id="@+id/logout3"
                android:layout_width="91dp"
                android:layout_height="53dp"
                android:layout_marginTop="28dp"
                android:background="@drawable/homekeyback"
                android:text="Find a location"
                android:textAlignment="center"
                android:textColor="@color/purple_500"
                android:textSize="18dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.361"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/logout" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>